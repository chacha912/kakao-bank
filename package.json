  "devDependencies": {
    "autoprefixer": "^10.2.5",
    "chokidar-cli": "^2.1.0",
    "gh-pages": "^3.1.0",
    "htmlhint": "^0.14.2",
    "live-server": "^1.2.1",
    "normalize-scss": "^7.0.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.13",
    "postcss-combine-media-query": "^1.0.1",
